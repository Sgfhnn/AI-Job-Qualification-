{
  "name": "ai-job-qualification-platform",
  "version": "1.0.0",
  "description": "AI-powered Job Qualification Platform MVP",
  "main": "server-fixed.js",
  "scripts": {
    "start": "node server-fixed.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "@types/node": "^20",
    "concurrently": "^8.2.2",
    "typescript": "^5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.10",
    "pdf-parse": "^2.4.5",
    "textract": "^2.5.0",
    "uuid": "^13.0.0"
  }
}
